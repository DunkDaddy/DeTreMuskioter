<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    style="@style/header"
    tools:context=".Tables">

    <Spinner
        android:id="@+id/planets_spinner3"
        style="@style/tables_Spinner"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/TableTest"
        tools:ignore="SpeakableTextPresentCheck" />

    <TextView
        android:id="@+id/TableTest"
        style="@style/tables_textView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button9"
        style="@style/button.tables"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/LisviewTest"
        app:layout_constraintVertical_bias="0.101" />

    <ListView
        android:id="@+id/LisviewTest"
        style="@style/listView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/planets_spinner3" />

</androidx.constraintlayout.widget.ConstraintLayout>